<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bmw" class="co.edureka.spring.core.beans.Car">
		<constructor-arg value="B M W"/>
		<constructor-arg value="4750000.0"/>
	</bean>
	
	<bean id="prv" class="co.edureka.spring.core.beans.User">
		<constructor-arg value="Praveen"/>
		<constructor-arg value="Kumar"/>
		<constructor-arg ref="bmw"/> 
	</bean>
</beans>
